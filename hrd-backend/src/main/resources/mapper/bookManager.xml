<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.skc.domain.bookmanager.BookManagerMapper">

  <select id="saveBookManager" resultType="BookManagerDTO">
	SELECT 
		IFNULL(MAX(custno), 0)+1 AS next_custno 
	FROM shop_member
  </select>
  

  
  
</mapper>